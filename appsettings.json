{
  "ConnectionStrings": {
    "DefaultConnection": "Server={DB_SERVER};Database={DB_NAME};User={DB_USER};Password={DB_PASSWORD};Port=3306;allowPublicKeyRetrieval=true"
  },
  "JwtSettings": {
    "Issuer": "alojine",
    "Audience": "Users",
    "Key": "{JWT_SECRET}"
  },
  "AppSettings": {
    "Token":  "{JWT_SECRET}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
